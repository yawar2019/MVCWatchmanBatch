
@{
    ViewBag.Title = "SendData2";
}

<h2>SendData2</h2>



 

<table class="table-bordered table-striped table-responsive">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in ViewBag.empInfo)//3
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }

</table>

